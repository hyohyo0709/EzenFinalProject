<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
    
    <mapper namespace="ezenproject2.dao.OrderDAO">
    
    <select id="getBooksInfo" parameterType="BookDTO">
    	  <![CDATA[
    	select book_title, book_price,book_img
    	from ezenbooks where book_title={#book_title}
  		 ]]>  
    </select>
    		
    
    
    </mapper>